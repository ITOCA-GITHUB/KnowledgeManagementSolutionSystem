@{
    ViewData["Title"] = "Upload";
}

<h2>Upload File</h2>

<form asp-action="Upload" asp-controller="FileUpload" method="post" enctype="multipart/form-data">
    <input type="file" name="file" />
    <button type="submit">Upload</button>
</form>

@if (ViewBag.Message != null)
{
    <p>@ViewBag.Message</p>
}

@if (ViewBag.GoogleDriveLink != null)
{
    <p>Google Drive Link: <a href="@ViewBag.GoogleDriveLink" target="_blank">@ViewBag.GoogleDriveLink</a></p>
}
